{
  "title": "Multiple Paste and Go Button",
  "name": "multiple-paste-and-go-button",
  "id": "multiple-paste-and-go-button@wantora.github.io",
  "version": "1.0.1",
  "description": "This add-on provides a button to open multiple URLs from clipboard.",
  "main": "index.js",
  "author": "wantora",
  "engines": {
    "firefox": ">=52.0"
  },
  "license": "MIT",
  "keywords": [
    "jetpack",
    "tab",
    "toolbar",
    "button",
    "paste"
  ],
  "homepage": "https://github.com/wantora/multiple-paste-and-go-button",
  "repository": {
    "type": "git",
    "url": "https://github.com/wantora/multiple-paste-and-go-button.git"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "npm-run-all --silent --sequential --continue-on-error cmd:webpack:dev cmd:jpm:run",
    "build": "npm-run-all --silent --sequential cmd:webpack:prod cmd:jpm:xpi",
    "cmd:jpm:run": "jpm run",
    "cmd:jpm:xpi": "jpm xpi",
    "cmd:webpack:dev": "webpack --config webpack.config.js",
    "cmd:webpack:prod": "webpack --config webpack.production.config.js"
  },
  "preferences": [
    {
      "name": "switchToNewTabs",
      "type": "bool",
      "title": "Switch to new tabs it immediately",
      "value": false
    },
    {
      "name": "heuristicMode",
      "type": "bool",
      "title": "Heuristic parsing of URL",
      "value": true
    },
    {
      "name": "additionalSchemes",
      "type": "string",
      "title": "Additional URL schemes",
      "description": "Comma separated text.",
      "value": ""
    }
  ],
  "permissions": {
    "private-browsing": true,
    "multiprocess": true
  },
  "dependencies": {
    "events": "^1.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "jpm": "^1.3.1",
    "npm-run-all": "^4.0.2",
    "web-ext": "^1.8.1",
    "webpack": "^2.3.2"
  },
  "hasEmbeddedWebExtension": true
}
