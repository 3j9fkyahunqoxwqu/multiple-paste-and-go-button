{
  "name": "multiple-paste-and-go-button",
  "description": "This add-on provides a button to open multiple URLs from clipboard.",
  "version": "2.4.1",
  "author": "wantora",
  "dependencies": {
    "events": "^1.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.2.3",
    "eslint": "^4.12.1",
    "eslint-config-wantora": "github:wantora/eslint-config-wantora#v1.0.0",
    "eslint-loader": "^1.9.0",
    "glob": "^7.1.2",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "web-ext": "^2.2.2",
    "webpack": "^3.9.1"
  },
  "homepage": "https://github.com/wantora/multiple-paste-and-go-button",
  "keywords": [
    "button",
    "jetpack",
    "paste",
    "tab",
    "toolbar"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wantora/multiple-paste-and-go-button.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm-run-all -s clean build:js build:ext",
