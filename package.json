{
  "name": "multiple-paste-and-go-button",
  "description": "This add-on provides a button to open multiple URLs from clipboard.",
  "version": "2.4.1",
  "author": "wantora",
  "dependencies": {
    "events": "^1.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "jpm": "^1.3.1",
    "npm-run-all": "^4.0.2",
    "web-ext": "^2.0.0",
    "webpack": "^2.3.2"
  },
  "homepage": "https://github.com/wantora/multiple-paste-and-go-button",
  "keywords": [
    "button",
    "jetpack",
    "paste",
    "tab",
    "toolbar"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wantora/multiple-paste-and-go-button.git"
  },
  "scripts": {
    "_web-ext:build": "web-ext build --source-dir dist",
    "_web-ext:run": "web-ext run --source-dir dist",
    "_webpack:prod": "webpack --config webpack.production.config.js",
    "_webpack:dev": "webpack --config webpack.config.js",
    "_webpack:watch": "npm run _webpack:dev -- --watch",
    "build": "npm-run-all -s _webpack:prod _web-ext:build",
    "lint": "eslint .",
    "start": "npm-run-all -s _webpack:dev -p --race _webpack:watch _web-ext:run"
  }
}
